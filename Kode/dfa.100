/* dfa.100 -- dfa for binary numbers divisible by 3 */

int main()
{
  int state;
  char c, *w;

  w = getstring(100);
  state = 0;
  while (((c = w*) == '0') + (c == '1')) {
    if (state==0) {
      if (c=='1') state = 1;
    }
    else if (state==1)
      if (c=='0') state = 2;
      else state = 0;
    else
      if (c=='0') state = 1;
      else state = 2;
    w = w+1;
  }
  putstring("Remainder is "); putint(state);
  return 0;
}